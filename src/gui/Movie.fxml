<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import org.controlsfx.control.Rating?>

<AnchorPane prefHeight="505.0" prefWidth="507.0" styleClass="filePathInput-box" stylesheets="@style2.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MovieController">
   <children>
      <HBox alignment="CENTER" layoutX="178.0" layoutY="80.0" prefHeight="59.0" prefWidth="176.0">
         <children>
            <TextField fx:id="filePath" alignment="CENTER" styleClass="bbbb" stylesheets="@style2.css">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </TextField>
         </children>
      </HBox>
      <Text layoutX="83.0" layoutY="165.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Movie Title" />
      <Text layoutX="88.0" layoutY="218.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Category" />
      <Text layoutX="59.0" layoutY="292.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Selected Categories" />
      <Text layoutX="70.0" layoutY="408.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Personal Rating" />
      <Text layoutX="89.0" layoutY="106.0" strokeType="OUTSIDE" strokeWidth="0.0" text="File path" />
      <TextField fx:id="movieTitle" layoutX="191.0" layoutY="148.0" />
      <Button fx:id="newCategory" layoutX="144.0" layoutY="201.0" mnemonicParsing="false" onAction="#newCategoryButton" text="New" />
      <ChoiceBox fx:id="categoryChoice" layoutX="191.0" layoutY="201.0" prefWidth="150.0" />
      <Button fx:id="addCategory" layoutX="380.0" layoutY="201.0" mnemonicParsing="false" onAction="#addCategoryButton" text="Add" />
      <ListView fx:id="selectedCategories" layoutX="191.0" layoutY="260.0" prefHeight="93.0" prefWidth="149.0" />
      <Button fx:id="removeCategory" layoutX="369.0" layoutY="260.0" mnemonicParsing="false" onAction="#removeCategoryButton" text="Remove" />
      <Button fx:id="openFile" layoutX="385.0" layoutY="89.0" mnemonicParsing="false" onAction="#openFileButton" text="...." />
      <Rating fx:id="personalRating" layoutX="178.0" layoutY="388.0" rating="0.0" stylesheets="@style.css" />
      <Button fx:id="saveMovie" layoutX="392.0" layoutY="448.0" mnemonicParsing="false" onAction="#saveMovieButton" text="Save" />
      <Text fill="#626060" layoutX="178.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Movie">
         <font>
            <Font name="Cambria Bold" size="21.0" />
         </font>
      </Text>
      <Text fill="#626060" layoutX="254.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Settings">
         <font>
            <Font name="Cambria" size="21.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
